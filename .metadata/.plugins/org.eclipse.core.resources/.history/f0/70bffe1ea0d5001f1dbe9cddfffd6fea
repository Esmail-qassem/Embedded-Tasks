#include "../inc/TWI_interface.h"
void TWI_voidMasterInit(void)
{
/*SCL CLOCK*/
TWBR_Reg =255;
TWSR_Reg->TWPSX=PRESCALER_BY_1;
/*ENABLE*/
TWCR_Reg->TWEN=1;
}

void TWI_voidStartCondition(void)
{
    TWCR_Reg->TWINT = 1;   // Clear the interrupt flag
    TWCR_Reg->TWSTA = 1;   // Start condition
    TWCR_Reg->TWEN = 1;    // Enable TWI
    while (TWCR_Reg->TWINT == 0);  // Wait for the operation to complete
}

void TWI_voidStopCondition(void)
{
	TWCR_Reg->TWINT=1;
	TWCR_Reg->TWSTO=1;
	TWCR_Reg->TWEN=1;

}
void TWI_voidSendData(uint8 Copy_u8Data)
{
TWDR_Reg=Copy_u8Data;
TWCR_Reg->TWINT=1;
TWCR_Reg->TWEN=1;
while(TWCR_Reg->TWINT==0);
}
uint8 TWI_u8ReceiveData(void)
{	TWCR_Reg->TWINT=1;
	TWCR_Reg->TWEN=1;
	while(TWCR_Reg->TWINT==0);
return TWDR_Reg;
}








