#include "../inc/TWI_interface.h"
#include "CLCD/inc/CLCD_interface.h"
void TWI_voidMasterInit(void)
{

	/*SCL CLOCK*/
TWBR_Reg =255;
TWSR_Reg->TWPSX=PRESCALER_BY_1;
/*ENABLE*/
TWCR_Reg->TWEN=1;
}

void TWI_voidStartCondition(void)
{

	TWCR_Reg->TWSTA=1;
	TWCR_Reg->TWEN=1;
	TWCR_Reg->TWINT=1;
while(TWCR_Reg->TWINT==0);
}
void TWI_voidStopCondition(void)
{
	TWCR_Reg->TWSTO=1;
	TWCR_Reg->TWEN=1;
	TWCR_Reg->TWINT=1;
}
void TWI_voidSendData(uint8 Copy_u8Data)
{
TWCR*Local_Variable=TWCR_Reg;
TWDR_Reg=Copy_u8Data;
//Local_Variable->TWINT=1;
Local_Variable->TWEN=1;
TWCR_Reg->TWCR_8bit_Reg=Local_Variable->TWCR_8bit_Reg;
CLCD_voidGoToXY(0,0);
CLCD_voidWriteNumber(Local_Variable->TWCR_8bit_Reg);

while(Local_Variable->TWINT==0);
}
uint8 TWI_u8ReceiveData(void)
{
	TWCR*Local_Variable;

	Local_Variable->TWEN=1;
	Local_Variable->TWINT=1;
	TWCR_Reg->TWCR_8bit_Reg=Local_Variable->TWCR_8bit_Reg;
	while(TWCR_Reg->TWINT==0);
return TWDR_Reg;
}








