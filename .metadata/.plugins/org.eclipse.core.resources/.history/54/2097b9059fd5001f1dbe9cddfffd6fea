#include  "EEPROM/inc/EEPROM_interface.h"
#include  <util/delay.h>
void static Test_Code(void)
{
static int i=0;
EEPROM_voidSendData(0x111,5);
_delay_ms(50);
//uint8 x=EEPROM_voidReceiveData(0x111);
CLCD_voidGoToXY(1,1);
CLCD_voidWriteNumber(i);



}
int main()
{
	TrafficLightInit();
	EEPROM_Init();


	while(1)
	{
		Test_Code();


		//TrafficLightMainFunction();
	}
}


