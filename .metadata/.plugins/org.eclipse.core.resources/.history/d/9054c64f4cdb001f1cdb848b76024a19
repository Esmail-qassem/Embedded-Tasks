#include "PORT/inc/PORT_interface.h"
#include "LCD_OLED/inc/OLED.h"
#include "DIO/inc/DIO_interface.h"

int main(void)
{

    PORT_voidInit();
    SSD1306_Init(); // Initialize the OLED display
    SSD1306_ClearDisplay(); // Clear the screen
    // Display the number 12345 on page 0, column 0

	  SSD1306_DisplayNumber(5+5, 0, 0);
	  SSD1306_DisplayChar('E', 2, 0*6); // Displays 'A' on page 0, column 0
	  SSD1306_DisplayChar('S', 2, 1*6); // Displays 'A' on page 0, column 0
	  SSD1306_DisplayChar('M', 2, 2*6); // Displays 'A' on page 0, column 0
	  SSD1306_DisplayChar('A', 2, 3*6); // Displays 'A' on page 0, column 0
	  SSD1306_DisplayChar('i', 2, 4*6); // Displays 'A' on page 0, column 0
	  SSD1306_DisplayChar('l', 2, 5*6); // Displays 'A' on page 0, column 0
	  SSD1306_DisplayString("Esmail!!", 1, 0);


      while (1)
      {
          // Your main loop code here
      }
return 0;
}


