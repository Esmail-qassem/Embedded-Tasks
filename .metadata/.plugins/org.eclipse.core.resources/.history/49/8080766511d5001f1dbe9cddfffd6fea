#include "TrafficLights/inc/TrafficLightsControl_interface.h"
#include  "Fixed_Point/inc/FixedPoint_interface.h"
float test(float a ,float b)
{
	float result = a*b;
	return result;
}
int main()
{
	TrafficLightInit();
	fixed16_t Test;
	float x,y;
	float result;


	while(1)
	{

//		Test= Fixed_Sub(11,10.5);
		DIO_SetPinVal(DIO_PORTC,DIO_Pin0,DIO_HIGH);
		for(int i =0;i<1000;++i)
		{float total;
		total=(float)i+0.3f;
			Test= Fixed_Mul(total,total);
		}

		DIO_SetPinVal(DIO_PORTC,DIO_Pin0,DIO_LOW);

		DIO_SetPinVal(DIO_PORTC,DIO_Pin1,DIO_HIGH);
		for(float i =0;i<1000;++i)
			{
			float total;
			total=(float)i+0.3f;
			result= test(i+0.5f,i+1.5f);
			}
		DIO_SetPinVal(DIO_PORTC,DIO_Pin1,DIO_LOW);
		CLCD_voidGoToXY(0,0);
		CLCD_voidWriteNumber(Test);


		//TrafficLightMainFunction();
	}
}


